{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.scpdy_gib.main": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": ["anim.look_at_target", "anim.moving"],
					"transitions": [
						{
							"remove": "v.remove"
						}
					]
				},
				"remove": {
					"animations": ["anim.moving", "anim.remove"]
				}
			}
		}
	}
}
